{
    "python.sortImports.path": "${env:HOME}/.local/venv/nvim/bin/isort",
    "python.formatting.provider": "black",
    "python.formatting.blackPath": "${env:HOME}/.local/venv/nvim/bin/black",
    "python.venvPath": "${env:HOME}/.local/venv/nvim",
    "pyright.enable": true,
    "python.pythonPath": "${env:HOME}/.local/venv/nvim/bin/python",
    "tabnine.disable_file_regex": [],
    "languageserver": {
        "golangci-lint-languageserver": {
            "command": "golangci-lint-langserver",
            "filetypes": [
                "go"
            ],
            "initializationOptions": {
                "command": [
                    "golangci-lint",
                    "run",
                    "--out-format",
                    "json",
                    "--issues-exit-code=1"
                ]
            }
        }
    },
    "diagnostic-languageserver.filetypes": {
        "go": "golangci-lint-languageserver"
    },
    "diagnostic-languageserver.linters": {
        "go": "golangci-lint-languageserver"
    },
    "snippets.ultisnips.pythonPrompt": false,
    "golines.args": [
        "-m",
        "120"
    ],
    "git.enableGutters": true
}
